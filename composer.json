{
  "name": "phpcommon/annotation",
  "type": "library",
  "description": "A simple and fast annotation reader for PHP 7+.",
  "keywords": ["phpcommon", "annotation"],
  "license": "MIT",
  "homepage": "https://github.com/marcospassos/phpcommon-annotation",
  "authors": [
    {
      "name": "Marcos Passos",
      "email": "marcos@marcospassos.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "doctrine/annotations": "^1.4",
    "phpunit/phpunit": "^6.2"
  },
  "require-dev": {
    "php": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "PhpCommon\\Annotation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpCommon\\Annotation\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
